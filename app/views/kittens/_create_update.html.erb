<%= form_with model: kitten do |f| %>
    <div>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>
    <div>
        <%= f.label :age %>
        <%= f.number_field :age , in: 0..20%>
    </div>
    <div>
        <%= f.label :cuteness %>
        <%= f.number_field :cuteness , in: 0..100%>
    </div>
    <div>
        <%= f.label :softness %>
        <%= f.number_field :softness , in: 0..100%>
    </div>
    <%= submit_tag %>
<% end %>